{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "amd": true,
        "jquery": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2017
    },
    "extends": "eslint:recommended",
    "rules": {
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-unused-vars": ["error", {"args" : "none"}],
        "no-console": "off"
    }
}
